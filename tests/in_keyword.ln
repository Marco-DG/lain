// Test: in keyword for index bounds - should compile successfully
// func get(arr int[], i int in arr) means i is always a valid index

func get(arr int[10], i int in arr) int {
    return arr[i]  // Always safe - compiler knows i âˆˆ [0, 9]
}

func main() int {
    mut arr int[10]
    arr[0] = 42
    arr[5] = 100
    
    mut x = get(arr, 5)  // OK: 5 is in [0, 10)
    mut y = get(arr, 0)  // OK: 0 is in [0, 10)
    
    return 0
}
