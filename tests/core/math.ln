extern proc libc_printf(fmt *u8, ...) int

proc main() int {
    var a = 10
    var b = 3
    libc_printf("10 + 3 = %d\n", a + b)
    libc_printf("10 - 3 = %d\n", a - b)
    libc_printf("10 * 3 = %d\n", a * b)
    libc_printf("10 / 3 = %d\n", a / b)
    libc_printf("10 %% 3 = %d\n", a % b)
    return 0
}
