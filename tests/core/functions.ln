extern proc libc_printf(fmt *u8, ...) int

func add(a int, b int) int {
    return a + b
}

proc fib(n int) int {
    if n < 2 { return n }
    return fib(n-1) + fib(n-2)
}

proc main() int {
    libc_printf("add(10, 20) = %d\n", add(10, 20))
    libc_printf("fib(10) = %d\n", fib(10))
    return 0
}
