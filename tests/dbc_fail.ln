// Test: constraint violation - should FAIL to compile
// Calling safe_div with 0 should violate b != 0 constraint

func safe_div(a int, b int != 0) int {
    return a / b
}

func main() int {
    var x = 10
    var y = 0
    var z = safe_div(x, y) // Should fail: y is 0, violating b != 0
    return 0
}
