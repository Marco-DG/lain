extern func printf(fmt u8[:0], ...) int

func main() int {
    var x = 10
    if x > 5 {
        printf("x > 5\n")
    } else {
        printf("x <= 5\n")
    }

    printf("Looping 0..5:\n")
    for i, v in 0..5 {
        printf("i=%d, v=%d\n", i, v)
    }

    var val = 2
    match val {
        1: printf("One\n")
        2: printf("Two\n")
        3..5: printf("Three to Five\n")
        else: printf("Other\n")
    }
    return 0
}
