import std.c
import std.fs

proc main() int {
    var msg_open = "Opening file: test_output.txt\n"
    libc_printf(msg_open.data)
    
    var path = "test_output.txt"
    var mode = "w"
    var f = open_file(path.data, mode.data)
    
    var msg_write = "Writing to file...\n"
    libc_printf(msg_write.data)
    
    var content = "Hello from Lain std/fs!\n"
    write_file(f, content.data)
    
    libc_printf("Closing file...\n")
    close_file(mov f)
    
    libc_printf("Done.\n")
    return 0
}
