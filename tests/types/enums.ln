// Simpler enum test - enum value directly without function

extern func libc_printf(fmt *u8, ...) int

type Color {
    Red,
    Green,
    Blue
}

func main() int {
    var c = Color.Red
    
    case c {
        Red: libc_printf("Color is Red\n")
        Green: libc_printf("Color is Green\n")
        Blue: libc_printf("Color is Blue\n")
        else: libc_printf("Unknown\n")
    }
    
    return 0
}
