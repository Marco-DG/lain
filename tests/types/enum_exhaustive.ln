// Test for enum exhaustiveness WITHOUT else
// All variants covered, so should compile

extern func printf(fmt *char, ...) int

type Status {
    Ready,
    Running,
    Done
}

func main() int {
    var s = Status.Running
    
    // All 3 variants covered, no else needed!
    match s {
        Ready: printf("Ready\n")
        Running: printf("Running\n")
        Done: printf("Done\n")
    }
    
    return 0
}
