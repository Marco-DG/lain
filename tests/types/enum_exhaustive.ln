// Test for enum exhaustiveness WITHOUT else
// All variants covered, so should compile

extern proc libc_printf(fmt *u8, ...) int

type Status {
    Ready,
    Running,
    Done
}

proc main() int {
    var s = Status.Running
    
    // All 3 variants covered, no else needed!
    case s {
        Ready: libc_printf("Ready\n")
        Running: libc_printf("Running\n")
        Done: libc_printf("Done\n")
    }
    
    return 0
}
