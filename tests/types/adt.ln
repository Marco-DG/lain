// Test: Algebraic Data Types (Unified Syntax)

extern proc libc_printf(fmt *u8, ...) int

type Shape {
    Circle { radius int }
    Rectangle { width int, height int }
    Point
}

proc main() int {
    var c = Shape.Circle(10)
    var r = Shape.Rectangle(10, 20)
    var p = Shape.Point
    
    case c {
        Circle(rad): libc_printf("Circle radius: %d\n", rad)
        Rectangle(w, h): libc_printf("Rect: %d x %d\n", w, h)
        Point: libc_printf("Point\n")
    }
    
    return 0
}
