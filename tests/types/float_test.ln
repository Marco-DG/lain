// Test: Float types (f32, f64)

extern proc libc_printf(fmt *u8, ...) int

proc main() int {
    var a f32 = 3.14
    var b f64 = 2.71828
    
    libc_printf("f32: %f\n", a)
    libc_printf("f64: %f\n", b)
    
    // Basic arithmetic
    var sum f64 = 10.5
    var diff f64 = 5.25
    libc_printf("sum: %f\n", sum)
    libc_printf("diff: %f\n", diff)
    
    return 0
}
